<% include ../partials/header %>
<form action="htttp://localhost:4000/even/add" method="post" class="col s12">
	<input type="hidden" name="id" value="">
	
	<div class="form-group row">
		<label for="itemname" class="col-2 col-form-label">Name</label>
		<div class="col-10">
			<input type="text" name="itemname" value="" class="form-control">
		</div>
	</div>

	<div class="form-group row">
		<label for="qty" class="col-2 col-form-label">Quantity</label>
		<div class="col-10">
			<input type="text" name="qty" value="" class="form-control">
		</div>
	</div>

	<div class="form-group row">
		<label for="amount" class="col-2 col-form-label">Amount</label>
		<div class="col-10">
			<input type="text" name="amount" value="" class="form-control">
		</div>
	</div>

	<div class="form-group row">
		<label for="amount" class="col-2 col-form-label" style="color:#ffffff"> Click Here to submit </label>
		<div class="col-10">
			<input type="submit" name="submit" value="Save event" class="btn btn-primary btn-lg">
		</div>
	</div>
</form>


<% include ../partials/footer %> 